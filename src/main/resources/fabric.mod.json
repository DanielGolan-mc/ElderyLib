{
  "schemaVersion": 1,
  "id": "elderly_lib",
  "version": "${version}",
  "name": "Elderly Library",
  "description": "A Library Required by Elderion Studio and Catalyst Studios",
  "authors": [
    "DanielGolan"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/elderly",
    "issues": "https://github.com/DanielGolan-mc/ElderyLib/issues",
    "sources": "https://github.com/DanielGolan-mc/ElderyLib",
    "discord": "https://discord.gg/9V8epgJWFC"
  },
  "license": "LGPL-2.1 License",
  "icon": "assets/elderly_lib/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.danielgolan.elderion.library.client.ElderlyClient"
    ],
    "main": [
      "net.danielgolan.elderion.library.Elderly"
    ]
  },
  "mixins": [
    "elderly_lib.mixins.json"
